{
  "name": "js-opensource-starwars",
  "version": "1.0.0",
  "description": "A js opensource demo studying",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "nyc mocha",
    "watch:test": "mocha --watch",
    "validate": "npm-run-all --parallel lint test"
  },
  "keywords": [
    "jindo",
    "star"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thanhlam1998/js-library-studying.git"
  },
  "author": "Thanh Lam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thanhlam1998/js-library-studying/issues"
  },
  "homepage": "https://github.com/thanhlam1998/js-library-studying#readme",
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^7.32.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "^1.3.2",
    "npm-run-all": "^2.3.0",
    "mocha": "^3.0.0",
    "nyc": "7.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
