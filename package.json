{
  "name": "js-opensource-starwars",
  "version": "1.0.0",
  "description": "A js opensource demo studying",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "test": "nyc mocha",
    "watch:test": "mocha --watch",
    "lint": "eslint src",
    "validate": "npm-run-all --parallel test lint build"
  },
  "keywords": [
    "jindo",
    "star"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thanhlam1998/js-library-studying.git"
  },
  "author": "Thanh Lam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thanhlam1998/js-library-studying/issues"
  },
  "homepage": "https://github.com/thanhlam1998/js-library-studying#readme",
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "eslint": "3.2.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "1.3.2",
    "mocha": "3.0.0",
    "npm-run-all": "2.3.0",
    "nyc": "7.1.0",
    "rimraf": "2.5.4"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
